{
  "name": "@abraxas/voting-ecollecting",
  "version": "0.0.0-development",
  "scripts": {
    "prepare": "husky install",
    "start": "(shx rm -rf dist/ecollecting-lib || true) && concurrently -k npm:start:lib npm:start:admin npm:start:citizen",
    "start:sta": "shx rm -rf dist/ecollecting-lib && concurrently -k npm:start:lib npm:start:admin:sta npm:start:citizen:sta",
    "start:lib": "ng build --project=ecollecting-lib --watch",
    "start:admin": "wait-on dist/ecollecting-lib && ng serve --project=admin --port 4600",
    "start:citizen": "wait-on dist/ecollecting-lib && ng serve --project=citizen --port 4700",
    "start:admin:sta": "wait-on dist/ecollecting-lib && ng serve --project=admin -c=local-sta --port 4600",
    "start:citizen:sta": "wait-on dist/ecollecting-lib && ng serve --project=citizen -c=local-sta --port 4700",
    "build:lib": "ng build --project=ecollecting-lib",
    "build:admin": "npm run build:lib && cd ./projects/admin && ng build --project=admin --configuration production",
    "build:citizen": "npm run build:lib && cd ./projects/citizen && ng build --project=citizen --configuration production",
    "lint": "prettier --check \"./projects/**/*.{ts,scss}\" && ng lint && stylelint \"./projects/**/*.scss\"",
    "lint:ng": "ng lint",
    "lint:license": "eslint --fix .",
    "fix": "prettier --write \"./projects/**/*.{ts,scss}\" && ng lint --fix && stylelint \"./projects/**/*.scss\" --fix"
  },
  "private": true,
  "dependencies": {
    "@abraxas/base-components": "20.19.5",
    "@abraxas/voting-ecollecting-proto": "1.96.2",
    "@abraxas/voting-lib": "0.0.1-dev.664",
    "@angular/animations": "20.3.13",
    "@angular/common": "20.3.13",
    "@angular/compiler": "20.3.13",
    "@angular/core": "20.3.13",
    "@angular/forms": "20.3.13",
    "@angular/platform-browser": "20.3.13",
    "@angular/platform-browser-dynamic": "20.3.13",
    "@angular/router": "20.3.13",
    "@ngx-translate/core": "17.0.0",
    "deepmerge": "4.3.1",
    "google-protobuf": "3.21.4",
    "grpc-web": "1.5.0",
    "lodash": "4.17.21",
    "moment": "2.30.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.11",
    "@angular-eslint/builder": "20.6.0",
    "@angular-eslint/eslint-plugin": "20.6.0",
    "@angular-eslint/eslint-plugin-template": "20.6.0",
    "@angular-eslint/schematics": "20.6.0",
    "@angular-eslint/template-parser": "20.6.0",
    "@angular/cli": "20.3.11",
    "@angular/compiler-cli": "20.3.13",
    "@types/lodash": "4.17.20",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "concurrently": "9.2.1",
    "eslint": "9.39.1",
    "eslint-plugin-license-header": "0.8.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "ng-lint-staged": "12.0.4",
    "ng-packagr": "20.3.2",
    "prettier": "3.6.2",
    "shx": "0.4.0",
    "stylelint": "16.25.0",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-scss": "6.12.1",
    "typescript": "5.9.3",
    "wait-on": "8.0.5"
  }
}
