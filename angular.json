{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/admin",
      "sourceRoot": "projects/admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "projects/admin/dist"
            },
            "index": "projects/admin/src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
                "moment",
                "moment/locale/de",
                "deep-equal",
                "file-saver",
                "lodash",
                "google-protobuf",
                "grpc-web",
                "google-protobuf/google/protobuf/empty_pb",
                "google-protobuf/google/protobuf/timestamp_pb",
                "deepmerge",
                "jsrsasign"
              ],
            "assets": [
              "projects/admin/src/favicon.ico",
              "projects/admin/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@abraxas/base-components/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@abraxas/voting-lib/assets",
                "output": "assets"
              },
              {
                "glob": "**/*",
                "input": "projects/ecollecting-lib/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "projects/admin/src/styles.scss"
            ],
            "loader": {
              ".ttf": "file",
              ".woff": "file",
              ".woff2": "file"
            },
            "scripts": [],
            "browser": "projects/admin/src/main.ts"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "local-sta": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/admin/src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.local-sta.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/admin/src/environments/environment.ts",
                  "with": "projects/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": {
                "styles": {
                  "inlineCritical": false
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "admin:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "admin:build:production"
            },
            "local-sta": {
              "buildTarget": "admin:build:local-sta"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/admin/src/**/*.ts",
              "projects/admin/src/**/*.component.html"
            ]
          }
        }
      }
    },
